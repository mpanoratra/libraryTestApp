<h1>Listing books</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Author</th>
      <th>Genre</th>
      <th>Quantity</th>
      <th>Number Checked Out</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= render @books %>
    <%= will_paginate @books %>
  </tbody>
</table>

<br>

<%= link_to 'New Book', new_book_path %>
